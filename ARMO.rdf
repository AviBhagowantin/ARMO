    </rdf:Description>
    <rdf:Description rdf:about="https://github.com/AviBhagowantin/ARMO#te">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="https://github.com/AviBhagowantin/ARMO#d">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="https://github.com/AviBhagowantin/ARMO#tm">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="https://github.com/AviBhagowantin/ARMO#s">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="https://github.com/AviBhagowantin/ARMO#r">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="https://github.com/AviBhagowantin/ARMO#sb">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="https://github.com/AviBhagowantin/ARMO#i">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="https://github.com/AviBhagowantin/ARMO#dod">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description>
        <swrla:isRuleEnabled rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</swrla:isRuleEnabled>
        <rdfs:comment></rdfs:comment>
        <rdfs:label>Assigning Defects to Team Members in a Sprint</rdfs:label>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
        <swrl:body>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                <rdf:first>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                        <swrl:propertyPredicate rdf:resource="https://github.com/AviBhagowantin/ARMO#hasTestExecution"/>
                        <swrl:argument1 rdf:resource="https://github.com/AviBhagowantin/ARMO#tc"/>
                        <swrl:argument2 rdf:resource="https://github.com/AviBhagowantin/ARMO#te"/>
                    </rdf:Description>
                </rdf:first>
                <rdf:rest>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                        <rdf:first>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                                <swrl:classPredicate rdf:resource="https://github.com/AviBhagowantin/ARMO#Defect"/>
                                <swrl:argument1 rdf:resource="https://github.com/AviBhagowantin/ARMO#d"/>
                            </rdf:Description>
                        </rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                <rdf:first>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                        <swrl:propertyPredicate rdf:resource="https://github.com/AviBhagowantin/ARMO#assignedToSprint"/>
                                        <swrl:argument1 rdf:resource="https://github.com/AviBhagowantin/ARMO#tm"/>
                                        <swrl:argument2 rdf:resource="https://github.com/AviBhagowantin/ARMO#s"/>
                                    </rdf:Description>
                                </rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                        <rdf:first>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                                <swrl:propertyPredicate rdf:resource="https://github.com/AviBhagowantin/ARMO#verifies"/>
                                                <swrl:argument1 rdf:resource="https://github.com/AviBhagowantin/ARMO#tc"/>
                                                <swrl:argument2 rdf:resource="https://github.com/AviBhagowantin/ARMO#r"/>
                                            </rdf:Description>
                                        </rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                                <rdf:first>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                                        <swrl:propertyPredicate rdf:resource="https://github.com/AviBhagowantin/ARMO#sprintContainsRequirement"/>
                                                        <swrl:argument1 rdf:resource="https://github.com/AviBhagowantin/ARMO#sb"/>
                                                        <swrl:argument2 rdf:resource="https://github.com/AviBhagowantin/ARMO#r"/>
                                                    </rdf:Description>
                                                </rdf:first>
                                                <rdf:rest>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                                        <rdf:first>
                                                            <rdf:Description>
                                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                                                <swrl:propertyPredicate rdf:resource="https://github.com/AviBhagowantin/ARMO#hasSprintBacklog"/>
                                                                <swrl:argument1 rdf:resource="https://github.com/AviBhagowantin/ARMO#s"/>
                                                                <swrl:argument2 rdf:resource="https://github.com/AviBhagowantin/ARMO#sb"/>
                                                            </rdf:Description>
                                                        </rdf:first>
                                                        <rdf:rest>
                                                            <rdf:Description>
                                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                                                <rdf:first>
                                                                    <rdf:Description>
                                                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                                                        <swrl:propertyPredicate rdf:resource="https://github.com/AviBhagowantin/ARMO#foundDuring"/>
                                                                        <swrl:argument1 rdf:resource="https://github.com/AviBhagowantin/ARMO#d"/>
                                                                        <swrl:argument2 rdf:resource="https://github.com/AviBhagowantin/ARMO#te"/>
                                                                    </rdf:Description>
                                                                </rdf:first>
                                                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                                            </rdf:Description>
                                                        </rdf:rest>
                                                    </rdf:Description>
                                                </rdf:rest>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </rdf:rest>
            </rdf:Description>
        </swrl:body>
        <swrl:head>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                <rdf:first>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                        <swrl:propertyPredicate rdf:resource="https://github.com/AviBhagowantin/ARMO#isFixedBy"/>
                        <swrl:argument1 rdf:resource="https://github.com/AviBhagowantin/ARMO#d"/>
                        <swrl:argument2 rdf:resource="https://github.com/AviBhagowantin/ARMO#tm"/>
                    </rdf:Description>
                </rdf:first>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            </rdf:Description>
        </swrl:head>
    </rdf:Description>
    <rdf:Description>
        <swrla:isRuleEnabled rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</swrla:isRuleEnabled>
        <rdfs:comment></rdfs:comment>
        <rdfs:label>Inferring Requirement Fulfillment from the Definition of Done</rdfs:label>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
        <swrl:body>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                <rdf:first>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                        <swrl:classPredicate rdf:resource="https://github.com/AviBhagowantin/ARMO#Increment"/>
                        <swrl:argument1 rdf:resource="https://github.com/AviBhagowantin/ARMO#i"/>
                    </rdf:Description>
                </rdf:first>
                <rdf:rest>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                        <rdf:first>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                <swrl:propertyPredicate rdf:resource="https://github.com/AviBhagowantin/ARMO#satisfies"/>
                                <swrl:argument1 rdf:resource="https://github.com/AviBhagowantin/ARMO#i"/>
                                <swrl:argument2 rdf:resource="https://github.com/AviBhagowantin/ARMO#dod"/>
                            </rdf:Description>
                        </rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                <rdf:first>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                                        <swrl:classPredicate rdf:resource="https://github.com/AviBhagowantin/ARMO#DefinitionOfDone"/>
                                        <swrl:argument1 rdf:resource="https://github.com/AviBhagowantin/ARMO#dod"/>
                                    </rdf:Description>
                                </rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                        <rdf:first>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                                <swrl:propertyPredicate rdf:resource="https://github.com/AviBhagowantin/ARMO#appliesTo"/>
                                                <swrl:argument1 rdf:resource="https://github.com/AviBhagowantin/ARMO#dod"/>
                                                <swrl:argument2 rdf:resource="https://github.com/AviBhagowantin/ARMO#r"/>
                                            </rdf:Description>
                                        </rdf:first>
                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </rdf:rest>
            </rdf:Description>
        </swrl:body>
        <swrl:head>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
